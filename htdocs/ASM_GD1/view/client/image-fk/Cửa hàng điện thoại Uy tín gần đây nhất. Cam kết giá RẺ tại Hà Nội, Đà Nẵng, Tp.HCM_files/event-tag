        var valueAttr_formatted_dung_o_button_mua_hang_10s = '';
        for (let item of valueAttr_dung_o_button_mua_hang_10s) {
            valueAttr_formatted_dung_o_button_mua_hang_10s += elementAttr_dung_o_button_mua_hang_10s + item;
        }
        var box_dung_o_button_mua_hang_10s = document.querySelectorAll(valueAttr_formatted_dung_o_button_mua_hang_10s);
        var currentScroll_dung_o_button_mua_hang_10s = 0;
        var nextScroll_dung_o_button_mua_hang_10s = 1;
        document.addEventListener('scroll', function () {
            if (box_dung_o_button_mua_hang_10s && isInViewport(box_dung_o_button_mua_hang_10s)) {
                if (secondsStay_dung_o_button_mua_hang_10s === 0 && !startCountTime_dung_o_button_mua_hang_10s) {
                    startCountTime_dung_o_button_mua_hang_10s = true;
                    currentScroll_dung_o_button_mua_hang_10s++;

                    secondsStayInterval_dung_o_button_mua_hang_10s = setInterval(function () {
                        secondsStay_dung_o_button_mua_hang_10s++;

                        if (parseInt('10') === secondsStay_dung_o_button_mua_hang_10s) {
                            appendEvent('dung_o_button_mua_hang_10s');
                        }
                    }, 1000);
                } else {
                    if ((currentScroll_dung_o_button_mua_hang_10s + 1) === nextScroll_dung_o_button_mua_hang_10s) {
                        currentScroll_dung_o_button_mua_hang_10s++;

                        secondsStayInterval_dung_o_button_mua_hang_10s = setInterval(function () {
                            secondsStay_dung_o_button_mua_hang_10s++;

                            if (parseInt('10') === secondsStay_dung_o_button_mua_hang_10s) {
                                appendEvent('dung_o_button_mua_hang_10s');
                            }
                        }, 1000);
                    }
                }
            } else {
                clearInterval(secondsStayInterval_dung_o_button_mua_hang_10s);
                if (currentScroll_dung_o_button_mua_hang_10s === nextScroll_dung_o_button_mua_hang_10s) {
                    nextScroll_dung_o_button_mua_hang_10s++;
                }
            }
        }, {
            passive: true
        });
