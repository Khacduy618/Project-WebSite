    function appendEvent(event, isGroupEvent = false) {
        let newScript = document.createElement('script');
        newScript.type = 'text/javascript';
        newScript.innerHTML = `mePuzz("track", "${event}");`
        document.body.appendChild(newScript);

        if (!isGroupEvent) {
