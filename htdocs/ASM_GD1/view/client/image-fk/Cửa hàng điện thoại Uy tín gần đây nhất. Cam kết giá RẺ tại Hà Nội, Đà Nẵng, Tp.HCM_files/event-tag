                    if (!list_group_event_executed.includes('khach_dang_ky_nhung_khong_dang_nhap')) {
                var checkEventAppended_khach_dang_ky_nhung_khong_dang_nhap = false; 
                                                    if (!checkEventAppended_khach_dang_ky_nhung_khong_dang_nhap) {
                        var checkMatching_khach_dang_ky_nhung_khong_dang_nhap0 = true;
                        var arr_event_khach_dang_ky_nhung_khong_dang_nhap0 = [];

                                                    arr_event_khach_dang_ky_nhung_khong_dang_nhap0.push('khach_khong_dang_nhap');
                                                    arr_event_khach_dang_ky_nhung_khong_dang_nhap0.push('khach_click_dang_ky_tai_khoan');
                        
                        for (let i = 0; i < arr_event_khach_dang_ky_nhung_khong_dang_nhap0.length; i++) {
                            if (!list_event_added.includes(arr_event_khach_dang_ky_nhung_khong_dang_nhap0[i])) {
                                checkMatching_khach_dang_ky_nhung_khong_dang_nhap0 = false;
                                break;
                            }
                        }

                        if (checkMatching_khach_dang_ky_nhung_khong_dang_nhap0) {
                            appendEvent('khach_dang_ky_nhung_khong_dang_nhap', true);
                            checkEventAppended_khach_dang_ky_nhung_khong_dang_nhap = true;
                            list_group_event_executed.push('khach_dang_ky_nhung_khong_dang_nhap');
<!--                            setCookie('MEPUZZ_LIST_GROUP_EVENT_EXECUTED', JSON.stringify(list_group_event_executed), tomorrow);-->
                        }
                                            }
                            }
            }

    function checkUrlChange() {
        if (window.location.href !== oldUrl) {
            oldUrl = window.location.href;

            list_scroll_executed = [];
            eventPageViewUrl();
            eventViewNPage();
            list_leave_web_executed = [];
            onPageNSeconds(true);
            checkVisitor();
        }
    }

    var oldUrl = window.location.href;
    setInterval(checkUrlChange, 1000);
})(window);