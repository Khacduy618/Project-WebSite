                        if ('btn btn-lg btn-login' === elementFilter) {
                            appendEvent('khach_khong_dang_nhap');
                            list_click_executed.push('khach_khong_dang_nhap');
                        }
                    }
                }
            }
                    if (!list_click_executed.includes('chua_dang_nhap')) {
                let elementFilter = false;

                if (parseInt('2') == click_id)
                    elementFilter = elementId;
                else if (parseInt('2') == click_class)
                    elementFilter = elementClass;
                else if (parseInt('2') == click_tag_name)
                    elementFilter = elementTagName;
                else if (parseInt('2') == click_url)
                    elementFilter = elementUrl;

                if (elementFilter) {
                    if ('not_contain' === contain) {
                        if (elementFilter.indexOf('btn btn-lg btn-login') != -1) {
                            appendEvent('chua_dang_nhap');
                            list_click_executed.push('chua_dang_nhap');
                        }
                    } else if ('not_contain' === not_contain) {
                        if (elementFilter.indexOf('btn btn-lg btn-login') == -1) {
                            appendEvent('chua_dang_nhap');
                            list_click_executed.push('chua_dang_nhap');
                        }
                    } else if ('not_contain' === equal) {
                        if ('btn btn-lg btn-login' === elementFilter) {
                            appendEvent('chua_dang_nhap');
                            list_click_executed.push('chua_dang_nhap');
                        }
                    }
                }
            }
                    if (!list_click_executed.includes('chua_dang_ky_tai_khoan')) {
                let elementFilter = false;

                if (parseInt('2') == click_id)
                    elementFilter = elementId;
                else if (parseInt('2') == click_class)
                    elementFilter = elementClass;
                else if (parseInt('2') == click_tag_name)
                    elementFilter = elementTagName;
                else if (parseInt('2') == click_url)
                    elementFilter = elementUrl;

                if (elementFilter) {
                    if ('not_contain' === contain) {
                        if (elementFilter.indexOf('form-group') != -1) {
                            appendEvent('chua_dang_ky_tai_khoan');
                            list_click_executed.push('chua_dang_ky_tai_khoan');
                        }
                    } else if ('not_contain' === not_contain) {
                        if (elementFilter.indexOf('form-group') == -1) {
                            appendEvent('chua_dang_ky_tai_khoan');
                            list_click_executed.push('chua_dang_ky_tai_khoan');
